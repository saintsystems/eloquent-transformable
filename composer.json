{
    "name": "saintsystems/eloquent-transformable",
    "description": "Work with your Laravel Eloquent models the way you want them to look, not as they are.",
    "keywords": [
        "laravel",
        "eloquent",
        "transform"
    ],
    "authors": [
        {
            "name": "Saint Systems"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "laravel/framework": "5.8.*",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "SaintSystems\\Eloquent\\Transformable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SaintSystems\\Eloquent\\Transformable\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
